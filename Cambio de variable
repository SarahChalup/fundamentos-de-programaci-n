#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
    int a, b, aux;
    cout<<"Por favor ingrese dos nÃºmeros enteros"<<endl;
    cin>>a;
    cin>>b;
    aux=b;
    b=a;
    a=aux;
    cout<<"El nuevo valor de a es:";
    cout<<a<<endl;
    cout<<"El nuevo valor de b es: ";
    cout<<b;

    getch();
}




Proceso CambioVariable
	leer a
	leer b
	aux<-a
	a<-b
	b<-aux
	Escribir "El valor de a es:" a
	Escribir "El valor de b es: " b
	
FinProceso
